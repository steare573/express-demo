{
  "esnext": true,
  "node": true,
  "laxcomma": true,
  "laxbreak": true,
  "camelcase": false,
  "indent": 2,
  "newcap": true,
  "quotmark": "single",
  "unused": "vars",
  "trailing": true,
  "proto": true,
  "maxcomplexity": 10,
  "multistr": true,
  "maxparams": 6,
  "maxlen": 80,
  "maxstatements": 25,
  "eqnull": true,
  "immed": true,
  "latedef": "nofunc",
  "nonew": true,
  "white": true,

  "globalstrict": false,
  "undef": true,
  "unused": true,
  "trailing": true,


  "globals": {
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false,
    "before": false,
    "after": false
  }
}
